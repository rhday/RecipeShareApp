<!DOCTYPE html>
<html>
  <head>
    <title>RecipeShareApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <div>
    <%= link_to "Sign Up", signup_path %> | <%= link_to "Login", login_path %> | <%= link_to "Logout", logout_path, method: "delete" %>
  </div>
    <%= yield %>
  </body>
</html>
